{
  "endpoint": "log-sink.internal/audit",
  "auth": {
    "type": "mtls",
    "mtls_cert": "<path-to-client-cert>",
    "mtls_key": "<path-to-client-key>",
    "ca_cert": "<path-to-ca-cert>"
  },
  "vault": {
    "address": "https://vault.mock.local:8200",
    "path": "audit/log_sink/creds",
    "secret_name": "audit-sink-creds",
    "role": "log-sink-role"
  },
  "retention_days": 365,
  "enable_request_id": true,
  "test_mode": true,
  "rotation": {
    "enabled": false,
    "rotation_window_days": 30
  }
}
